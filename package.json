{
  "name": "rfebm-api",
  "version": "2.1.0",
  "private": true,
  "type": "module",
  "author": "Gabriel Trabanco Llano",
  "description": "Convert rfebm data to json",
  "license": "MIT",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "prepare": "husky install && relative-deps",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "test": "bun test",
    "lint": "eslint --ext .ts,.js .",
    "lint:staged": "lint-staged",
    "format:code": "prettier --write .",
    "build:binary": "rm -f rfebm-api && bun build --compile ./src/index.ts --outfile rfebm-api"
  },
  "dependencies": {
    "@elysiajs/cors": "0.7.2",
    "@elysiajs/static": "0.7.1",
    "@elysiajs/swagger": "0.7.4",
    "@gtrabanco/bun-rfebm-scraper-library": "1.1.6",
    "elysia": "0.7.30",
    "elysia-rate-limit": "2.0.1"
  },
  "devDependencies": {
    "@types/bun": "1.0.0",
    "bun-types": "1.0.20",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.1",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "relative-deps": "1.0.7",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "typescript": ">=5.2.2"
  },
  "engines": {
    "bun": "1.0.19"
  },
  "relativeDependencies": {
    "@gtrabanco/bun-rfebm-scraper-library": "../bun-rfebm-scraper-library"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "bun lint"
  },
  "keywords": [
    "handball",
    "balonmano",
    "spain",
    "espa√±a",
    "classification",
    "clasificacion",
    "equipos",
    "jugadores",
    "entrenadores",
    "beach",
    "playa"
  ]
}
